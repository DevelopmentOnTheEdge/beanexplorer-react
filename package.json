{
  "name": "beanexplorer-react",
  "version": "0.1.13",
  "description": "React component for BeanExplorer beans",
  "main": "dist/beanexplorer-react.js",
  "scripts": {
    "start": "webpack-dev-server --hot --history-api-fallback --open --progress --profile --colors",
    "example": "webpack --config webpack.example.config.js",
    "build": "webpack --config webpack.production.config.js --env.build --display-modules",
    "build-min": "webpack --config webpack.production.config.js --env.build --env.min --display-modules",
    "test": "jest",
    "watch": "npm run test -- --watch",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run-script coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevelopmentOnTheEdge/beanexplorer-react"
  },
  "author": "",
  "dependencies": {
    "json-pointer": "^0.6.0",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-datetime": "^2.8.10",
    "react-dom": "^15.6.1",
    "react-select": "^1.0.0-rc.5",
    "react-virtualized-select": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "3.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.2",
    "enzyme": "^2.8.2",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.3",
    "mocha": "3.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "^0.18.0",
    "webpack": "2.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1",
    "yargs": "7.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  }
}

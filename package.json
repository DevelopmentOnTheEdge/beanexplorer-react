{
  "name": "beanexplorer-react",
  "version": "1.0.41",
  "description": "React component for BeanExplorer beans",
  "main": "dist/beanexplorer-react.js",
  "jsnext:main": "dist/beanexplorer-react.es.js",
  "module": "dist/beanexplorer-react.es.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --host 127.0.0.1 --inline --hot --history-api-fallback --open --open-page \"beanexplorer-react/\" --progress --profile --colors",
    "server": "webpack-dev-server --host 127.0.0.1 --progress --profile --colors",
    "test": "jest",
    "watch": "npm run test -- --watch",
    "coverage": "npm test -- --coverage",
    "coveralls": "npm run-script coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "up": "npm version patch && npm publish",
    "example": "BABEL_ENV=production webpack --config webpack.example.config.js",
    "lib": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevelopmentOnTheEdge/beanexplorer-react"
  },
  "author": "Innokentiy Krivonosov",
  "dependencies": {
    "big-integer": "^1.6.48",
    "big-rational": "^0.10.7",
    "ckeditor4-react": "^1.2.0",
    "classnames": "^2.2.6",
    "isomorphic-fetch": "^2.2.1",
    "json-pointer": "^0.6.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-datetime": "^3.0.4",
    "react-dom": "16.14.0",
    "react-maskedinput": "^4.0.1",
    "react-select": "1.3.0",
    "react-virtualized-select": "3.1.0"
  },
  "devDependencies": {
    "ajv": "^6.12.6",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap-icons": "^1.0.0",
    "chai": "3.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.21.5",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^24.9.0",
    "mocha": "3.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "^16.14.0",
    "regenerator-runtime": "^0.10.5",
    "rollup": "0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-image-base64": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "style-loader": "^0.18.0",
    "universal-cookie": "^2.1.2",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^3.11.0",
    "yargs": "7.1.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "globals": {
      "NODE_ENV": "test"
    },
    "setupFiles": [
      "<rootDir>/test/jestGlobals.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  }
}
